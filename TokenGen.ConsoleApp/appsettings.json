{
  "Logging": {
    "LogLevel": {
      "Default": "trace"
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    }
  },
  "ApiLimitSetting": {
    "MaxRateLimit": 10,
    "RestDuration": "0:00:30",
    "WatchDuration": "0:00:30",
    "MaxForDuration": 100,
    "Blocking": false,
    "ApiServer": "Pretend Server"
  }
}
